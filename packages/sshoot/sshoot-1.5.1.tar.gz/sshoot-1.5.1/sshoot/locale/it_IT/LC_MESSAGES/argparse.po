#: /usr/lib/python3.6/argparse.py:304
msgid "usage: "
msgstr "utilizzo: "

#: /usr/lib/python3.6/argparse.py:835
msgid ".__call__() not defined"
msgstr "__call__() non definito"

#: /usr/lib/python3.6/argparse.py:1132
msgid "unknown parser %(parser_name)r (choices: %(choices)s)"
msgstr "parser %(parser_name)r sconosciuto (scelte: %(choices)s)"

#: /usr/lib/python3.6/argparse.py:1186
msgid "argument \"-\" with mode %r"
msgstr "argoment \"-\" con modo %r"

#: /usr/lib/python3.6/argparse.py:1194
msgid "can't open '%s': %s"
msgstr "impossibile aprire '%s': %s"

#: /usr/lib/python3.6/argparse.py:1398
msgid "cannot merge actions - two groups are named %r"
msgstr "impossibile unire le azioni - due gruppi con nome %r"

#: /usr/lib/python3.6/argparse.py:1436
msgid "'required' is an invalid argument for positionals"
msgstr "'required' è un argomento non valido per posizionali"

#: /usr/lib/python3.6/argparse.py:1458
msgid "invalid option string %(option)r: must start with a character %(prefix_chars)r"
msgstr "stringa di opzione %(option)r invalida: deve inziare con un caratter %(prefix_chars)r"

#: /usr/lib/python3.6/argparse.py:1478
msgid "dest= is required for options like %r"
msgstr "dest= è obbligatorio per opzioni come %r"

#: /usr/lib/python3.6/argparse.py:1495
msgid "invalid conflict_resolution value: %r"
msgstr "valore invalido per conflict_resolution: %r"

#: /usr/lib/python3.6/argparse.py:1579
msgid "mutually exclusive arguments must be optional"
msgstr "argomenti mutuamente esclusivi devono esser opzionali"

#: /usr/lib/python3.6/argparse.py:1642
msgid "positional arguments"
msgstr "argomenti posizionali"

#: /usr/lib/python3.6/argparse.py:1643
msgid "optional arguments"
msgstr "argomenti opzionali"

#: /usr/lib/python3.6/argparse.py:1658
msgid "show this help message and exit"
msgstr "mostra questo messaggio di aiuto ed esci"

#: /usr/lib/python3.6/argparse.py:1689
msgid "cannot have multiple subparser arguments"
msgstr "impossibile avere argomenti multipli per un subparser"

#: /usr/lib/python3.6/argparse.py:1741
msgid "unrecognized arguments: %s"
msgstr "argomento sconosciuto: %s"

#: /usr/lib/python3.6/argparse.py:1838
msgid "not allowed with argument %s"
msgstr "non permesso con argomento %s"

#: /usr/lib/python3.6/argparse.py:1884 /usr/lib/python3.6/argparse.py:1898
msgid "ignored explicit argument %r"
msgstr "argomento esplicito %r ignorato"

#: /usr/lib/python3.6/argparse.py:2005
msgid "the following arguments are required: %s"
msgstr "i seguenti argomenti sono necessari: %s"

#: /usr/lib/python3.6/argparse.py:2020
msgid "one of the arguments %s is required"
msgstr "uno degli argomenti %s è richiesto"

#: /usr/lib/python3.6/argparse.py:2063
msgid "expected one argument"
msgstr "un argomento atteso"

#: /usr/lib/python3.6/argparse.py:2064
msgid "expected at most one argument"
msgstr "atteso almeno un argomento"

#: /usr/lib/python3.6/argparse.py:2065
msgid "expected at least one argument"
msgstr "atteso al massimo un argomento"

#: /usr/lib/python3.6/argparse.py:2127
msgid "ambiguous option: %(option)s could match %(matches)s"
msgstr "opzione ambigua: %(option)s può corrispondere a %(matches)s"

#: /usr/lib/python3.6/argparse.py:2190
msgid "unexpected option string: %s"
msgstr "stringa di opzione non attesa: %s"

#: /usr/lib/python3.6/argparse.py:2294
msgid "%r is not callable"
msgstr "%r non è chiamabile"

#: /usr/lib/python3.6/argparse.py:2311
msgid "invalid %(type)s value: %(value)r"
msgstr "valore di %(type)s invalido: %(value)r"

#: /usr/lib/python3.6/argparse.py:2322
msgid "invalid choice: %(value)r (choose from %(choices)s)"
msgstr "scelta invalida: %(value)r (scelta tra %(choices)s)"

#: /usr/lib/python3.6/argparse.py:2398
msgid ""
"%(prog)s: error: %(message)s\n"
msgstr "%(prog)s: errore: %(message)s\n"
