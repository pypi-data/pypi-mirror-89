{% extends "auth/admin.base.html.j2" %}


{% block appcontent %}
  {{ field('ID', user.id) }}
  {{ field('Name', user.name) }}
  {{ field('Username', user.username) }}
  {{ field('Locale', kbd(user.locale) ~ config['RUNE_LANGUAGES'][user.locale]) }}
  {{ field('Birthdate', moment(user.birthdate).format('LL') if user.birthdate) }}
  {{ field('Roles', '<br />'.join(user.roles)) }}
{% endblock %}
